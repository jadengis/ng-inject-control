{
  "version": 2,
  "projects": {
    "ng-inject-control": {
      "projectType": "library",
      "root": "packages/ng-inject-control",
      "sourceRoot": "packages/ng-inject-control/src",
      "prefix": "ng-inject-control",
      "targets": {
        "build": {
          "executor": "@nrwl/angular:package",
          "outputs": ["dist/packages/ng-inject-control"],
          "options": {
            "project": "packages/ng-inject-control/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "packages/ng-inject-control/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "packages/ng-inject-control/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/ng-inject-control"],
          "options": {
            "jestConfig": "packages/ng-inject-control/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "packages/ng-inject-control/src/**/*.ts",
              "packages/ng-inject-control/src/**/*.html"
            ]
          },
          "outputs": ["{options.outputFile}"]
        }
      },
      "tags": []
    }
  }
}
